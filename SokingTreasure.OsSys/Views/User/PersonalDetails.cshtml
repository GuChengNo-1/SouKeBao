        //将时间戳转换为日期时间
        function fmtDate(obj) {
            var dateVal = obj + "";
            if (obj != null) {
                var date = new Date(parseInt(dateVal.replace("/Date(", "").replace(")/", ""), 10));
                var month = date.getMonth() + 1 < 10 ? "0" + (date.getMonth() + 1) : date.getMonth() + 1;
                var currentDate = date.getDate() < 10 ? "0" + date.getDate() : date.getDate();
                var hours = date.getHours() < 10 ? "0" + date.getHours() : date.getHours();
                var minutes = date.getMinutes() < 10 ? "0" + date.getMinutes() : date.getMinutes();
                var seconds = date.getSeconds() < 10 ? "0" + date.getSeconds() : date.getSeconds();
                return date.getFullYear() + "-" + month + "-" + currentDate;
                //+ " " + hours + ":" + minutes + ":" + seconds
            }
        }
    </script>
    <script type="text/javascript">
        $(function () {
            var userId;
            var regexEmail = /^([A-Za-z0-9_\-\.])+@@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/,
                regexPhone = /^(13[0-9]|14[579]|15[0-3,5-9]|16[6]|17[0135678]|18[0-9]|19[89])\d{8}$/;
            $.ajax({
                url: "/User/GetUserData",
                async: true,
                type: "get",
                dataType: "json",
                success: function (data) {
                    userId = data.data.Id;
                    $("#UserRealName").text(data.data.UserRealName);
                    $("#LoginName").text(data.data.LoginName);
                    $("#UserAge").text(data.data.UserAge);
                    $("#UserSex").text(data.data.UserSex);
                    $("#UserAddress").text(data.data.UserAddress);
                    $("#UserEmail").text(data.data.UserEmail);
                    $("#UserPhone").text(data.data.UserPhone);
                }
            })
            $.ajax({
                url: "/User/GetUserData",
                async:true,
                type: "get",
                dataType: "json",
                success: function (data) {
                    $("#form-UserRealName").val(data.data.UserRealName);
                    $("#form-UserAge").val(data.data.UserAge);
                    data.data.UserSex == "男" ? $("input[name='form-UserSex'][value='男']").attr("checked", true) : $("input[name='form-UserSex'][value='女']").attr("checked", true);
                    $("#form-UserAddress").val(data.data.UserAddress);
                    $("#form-UserEmail").val(data.data.UserEmail);
                    $("#form-UserPhone").val(data.data.UserPhone);
                }
            })
            $("#user-confirm-btn").click(function () {
                var sex = $('#form-UserSex:checked').val();
                var userData = {
                    UserAddress: $("#form-UserAddress").val(), UserRealName: $("#form-UserRealName").val(), UserAge: $("#form-UserAge").val(), Id:userId,
                    UserEmail: $("#form-UserEmail").val(), UserPhone: $("#form-UserPhone").val(), UserSex: sex == "男" ? "男" : "女"
                };
                console.log(userData);
                if (!regexEmail.test(userData.UserEmail)) {
                    layer.msg("请输入正确的邮箱", { icon: 7, time: 2000, shade: 0.4 });
                    return;
                }
                if (!regexPhone.test(userData.UserPhone)) {
                    layer.msg("请输入正确的手机号码", { icon: 7, time: 2000, shade: 0.4 });
                    return;
                }
                $.ajax({
                    url: "/User/UpdateUserInfo",
                    data:userData,
                    type: "post",
                    dataType:"json",
                    success: function (data) {
                        if (data.success) {
                            layer.msg("修改成功，赶紧刷新查看吧", { icon: 6, time: 2000, shade: 0.4 });
                        }
                        else if (data.success == false) {
                            layer.msg("修改失败", { icon: 5, time: 2000, shade: 0.4 });
                        }
                    }
                })
            })
        })
    </script>
