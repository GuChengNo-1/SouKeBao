    <script>
        $(function () {
            //导航栏
            layui.use('element', function () {
                var element = layui.element;
            });

            var limitcount = 5;
            var curnum = 1;
            function productsearch(start, limit) {
                layui.use(['table', 'laypage'], function () {
                    var table = layui.table,
                        laypage = layui.laypage;
                    var index = layui.load(2);
                    table.render({
                        elem: '#shareholder-table',
                        url: '/Shareholder/ShareholderListShow?index=' + curnum + "&&limit" + limitcount,
                        id: 'copyright',
                        loading: true,
                        defaultToolbar: ['filter', 'print', 'exports'],
                        cols: [[
                            { field: 'NumberId', title: '序号', width: 80, sort: true, fixed: 'left', unresize: true }
                            , { field: 'ShareholderName', title: '股东', width: 120, unresize: true }
                            , { field: 'CapitalKey', title: '出资比例', width: 150, unresize: true }
                            , { field: 'Contributive', title: '认缴出资', width: 150, unresize: true }
                            , { field: 'ContributiveTime', title: '出资时间', width: 150, unresize: true }
                        ]]
                        , page: false
                        , done: function (res) {
                            laypage.render({
                                elem: 'layui-paging',
                                count: res.tatol,
                                curr: curnum,
                                limit: limitcount,
                                layout: ['prev', 'page', 'next', 'skip', 'count', 'limit'],
                                jump: function (obj, first) {
                                    if (!first) {
                                        curnum = obj.curr;
                                        limitcount = obj.limit;
                                        productsearch(curnum, limitcount);
                                    }
                                    layer.close(index);
                                }
                            });
                        }
                    });

                    //查询股东
                    $('#where-shareholderName').click(function () {
                        table.reload('shareholder', {
                            where: {
                                ShareholderName: $('#where-shareholderName').val(),
                            },
                            page: false,
                            done: function (res) {
                                laypage.render({
                                    elem: 'layui-paging',
                                    count: res.tatol,
                                    curr: curnum,
                                    limit: limitcount,
                                    layout: ['prev', 'page', 'next', 'skip', 'count', 'limit'],
                                    jump: function (obj, first) {
                                        if (!first) {
                                            curnum = obj.curr;
                                            limitcount = obj.limit;
                                            productsearch(curnum, limitcount);
                                        }
                                        layer.close(index);
                                    }
                                });
                            }
                        })

                    })

                })

            }

        });
    </script>
